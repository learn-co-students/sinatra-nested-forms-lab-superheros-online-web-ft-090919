<h1><%= @team.name %></h1><br>
<h1>Team Motto: <%= @team.motto %></h1><br>

<% @superhero.each do |superhero| %>
  <div class="team">
    <h1>Hero Name: <%= superhero.name %></h1><br>
    <p>Hero Power: <%= superhero.power %></p><br>
    <p>Hero Biography: <%= superhero.biography %></p><br>
  </div><br>
<% end %>
